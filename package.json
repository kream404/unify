{
  "name": "unify",
  "version": "0.0.0",
  "main": "dist/unify/index.js",
  "scripts": {
    "ng": "ng serve --ssl",
    "clean": "rimraf dist",
    "electron": "wait-on https://localhost:4200 && electron .",
    "prestart": "npm run clean",
    "start": "concurrently -k \"ng serve --ssl\" \"npm run electron\"",
    "prebuild": "npm run clean",
    "build": "ng build",
    "test": "ng test",
    "prerelease": "npm run build -- --configuration production",
    "release": "electron-builder",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "build": {
    "appId": "com.electron.unify",
    "productName": "unify",
    "files": [
      "dist/unify"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.3",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.3",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@auth0/auth0-angular": "^1.10.1",
    "@openid/appauth": "^1.3.1",
    "querystring-es3": "^0.2.1",
    "randomstring": "^1.2.2",
    "rxjs": "~7.5.0",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.6.4",
    "@types/randomstring": "^1.1.8",
    "concurrently": "^7.3.0",
    "electron": "^20.0.1",
    "electron-builder": "^5.5.0",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "~4.7.2",
    "wait-on": "^6.0.1"
  }
}
